<% function _renderToc(items) { %>
    <ul class="toc">
        <% for (var i = 0 ; i < items.length ; i++) { %>
            <li>
                <a href="#<%= items[i].id %>"><%- items[i].text %></a>
                <% if (items[i].children.length) { %>
                    <%- _renderToc(items[i].children) %>
                <% } %>
            </li>
        <% } %>
    </ul>
<% } %>
<% if (typeof toc != "undefined") { %>
    <%- _renderToc(toc) %>
<% } %>
